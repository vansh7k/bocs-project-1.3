/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.

    Nand(a=sel, b=sel, out=notsel);
    Nand(a=a, b=notsel, out=apart);
    Nand(a=b, b=sel, out=bpart);
    Nand(a=apart, b=bpart, out=out);
}